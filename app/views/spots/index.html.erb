<h1>Edit Spots</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Address</th>
    <th>Description</th>
    <th>Location type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @spots.each do |spot| %>
  <tr>
    <td><%= spot.title %></td>
    <td><%= spot.latitude %></td>
    <td><%= spot.longitude %></td>
    <td><%= spot.address %></td>
    <td><%= spot.description %></td>
    <td><%= spot.location_type %></td>
    <td><%= link_to 'Show', spot %></td>
    <td><%= link_to 'Edit', edit_spot_path(spot) %></td>
    <td><%= link_to 'Destroy', spot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br>

<div><%= link_to 'Home', root_path %></div>
<div><%= link_to 'New Spot', new_spot_path %></div>




